{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": {
      "webpack": { "config": ".webpack.server.json" }
    }
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-trailing-spaces": 0,
    "import/no-unresolved": [2, "case-sensitive"]
  }
}
